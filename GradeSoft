score = input("Enter Score: ")
#check score type
try:
    score = float(score)
except:
    print("Error, Please enter number 0~1")
#check score range
if score > 1 or score < 0:
    print("Error, Please enter number 0~1")
#condition
elif score >= 0.9:
    print("A")
elif score >= 0.8:
    print("B")
elif score >= 0.7:
    print("C")
else:
    print("F")
